<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pahlawanindonesia"
    android:onClick="openAlbertus"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.pahlawan.biographypahlawanindonesia.CutnyakdhienActivity" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="290dp"
            android:layout_height="919dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="136dp"
                android:layout_height="35dp"
                android:onClick="openCutnyak"
                android:text="Cut Nyak Dhien"
                android:textColor="#000000" />

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:onClick="openCutnyak"
                android:src="@drawable/cutnyakdhien" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="136dp"
                android:layout_height="32dp"
                android:onClick="openHamengkubuwono"
                android:text="hamengkubuwonoix"
                android:textColor="#000000" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:onClick="openHamengkubuwono"
                android:src="@drawable/hamengkubuwonoix" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="136dp"
                android:layout_height="35dp"
                android:onClick="openAlbertus"
                android:text="albertus"
                android:textColor="#000000" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:onClick="openAlbertus"
                android:src="@drawable/albertus" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="136dp"
                android:layout_height="35dp"
                android:onClick="openSudirman"
                android:text="sudirman"
                android:textColor="#000000" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:onClick="openSudirman"
                android:src="@drawable/sudirman" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="136dp"
                android:layout_height="35dp"
                android:onClick="openAntasari"
                android:text="pangeranantasari"
                android:textColor="#000000" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:onClick="openAntasari"
                android:src="@drawable/pangeranantasari" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="136dp"
                android:layout_height="35dp"
                android:onClick="openSisingamangaraja"
                android:text="sisingamangarajaxii"
                android:textColor="#000000" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:onClick="openSisingamangaraja"
                android:src="@drawable/sisingamangarajaxii" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="136dp"
                android:layout_height="35dp"
                android:onClick="openOerip"
                android:text="oeripsoemaharjo"
                android:textColor="#000000" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:onClick="openOerip"
                android:src="@drawable/oeripsoemaharjo" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="136dp"
                android:layout_height="35dp"
                android:onClick="openPakubuwono"
                android:text="pakubuwonox"
                android:textColor="#000000" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:onClick="openPakubuwono"
                android:src="@drawable/pakubuwonox" />

        </LinearLayout>
    </ScrollView>

</RelativeLayout>
